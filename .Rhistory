calculateUtility <- function(alpha, theta) {
q <- seq(2, 30, by=2)
df <- data.frame(q, sapply(q, function(x) theta * x))
names(df) <- c('q','transfers')
df['utility'] <- (1-exp(-alpha*df$q))/alpha - df$transfers
df
}
calculateUtility(0.1, 0.1)
